<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/Teachers">Teachers Management</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Teacher</li>
  </ol>
</nav>

<div class="view-user p-5">
  {{#each rows}}
    <div class="row mb-5">
      <div class="col text-center">
        <h3>{{this.first_name}} {{this.last_name}}</h3>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">First Name</th>
              <th scope="col">Middle Name</th>
              <th scope="col">Last Name</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">{{this.first_name}}</th>
              <td>{{this.middle_name}}</td>
              <td>{{this.last_name}}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  {{/each}}
</div>